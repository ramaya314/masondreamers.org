  "scripts": {
    "test": "node scripts/test.js --env=jsdom",
    "babelify": "NODE_ENV=production babel src --out-dir babel-src --copy-files --ignore '**/*.test.js'",
    "start": "node scripts/start.js",
    "start:server": "node server/index.js",
    "start:prod:server": "NODE_ENV=production node server/index.js",
    "build:server": "npm run babelify",
    "build:server:start": "npm run build:server && npm run start:server",
    "build:server:prod:start": "npm run build:server && npm run start:prod:server",
    "build:client": "node scripts/build.js",
    "build:client:server": "npm run build:client && npm run build:server",
    "build:client:server:start": "npm run build:client:server && npm run start:server",
    "build:client:server:prod:start": "npm run build:client:server && npm run start:prod:server",
    "clean": "rimraf build babel-src"
  },
  "jest": {
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/FileStub.js",
      "^.+\\.css$": "<rootDir>/config/jest/CSSStub.js"
    },
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|docs|node_modules)/"
    ],
    "testEnvironment": "node"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
